                                        <fileTypes>
                                                <filetype name="Analyze Image" extension="img" description="Analyze Image file">
                                                        <need>hdr</need>
                                                </filetype>
                                        </fileTypes>
                                </format>
                        </input>
                        <output id="HemisphereSplit_1.LeftSurface" name="Left Surface" description="output surface file path, left hemisphere" required="true" enabled="true" order="2" switch="--left" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="dfs" extension="dfs" description="" />
                                        </fileTypes>
                                </format>
                        </output>
                        <output id="HemisphereSplit_1.RightSurface" name="Right Surface" description="output surface file path, right hemisphere" required="true" enabled="true" order="3" switch="--right" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="dfs" extension="dfs" description="" />
                                        </fileTypes>
                                </format>
                        </output>
                        <input id="HemisphereSplit_1.Verbosity" name="Verbosity" description="verbosity (0 = silent) [default: 1]" required="false" enabled="true" order="7" switch="-v" switchSpaced="true">
                                <format type="Enumerated" cardinality="1">
                                        <enumeration>0</enumeration>
                                        <enumeration>1</enumeration>
                                        <enumeration>2</enumeration>
                                </format>
                                <values>
                                        <value>2</value>
                                </values>
                        </input>
                        <input id="HemisphereSplit_1.Timer" name="Timer" description="Show timing function" required="false" enabled="true" order="8" switch="--timer" switchSpaced="true">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="HemisphereSplit_1.PialSurface" name="Pial Surface" description="pial surface file -- must have same geometry as WM surface" required="false" enabled="true" order="4" switch="-p" switchSpaced="true">
