                        <input id="PartialVolumeClassifier_1.InputMRIFile" name="Input MRI File" description="Brain Only Volume" required="true" enabled="true" order="0" switch="-i" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Analyze Image" extension="img" description="Analyze Image file">
                                                        <need>hdr</need>
                                                </filetype>
                                        </fileTypes>
                                </format>
                        </input>
                        <input id="PartialVolumeClassifier_1.MaskFile" name="Mask File" description="An 8-bit file delineating brain/non-brain voxels; if not specified, PVC will assume non-zero voxels are foreground" required="false" enabled="false" order="2" switch="-m" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Analyze Image 3D" extension="img" description="Analyze Image 3D file">
                                                        <need>hdr</need>
                                                </filetype>
                                        </fileTypes>
                                </format>
                        </input>
                        <input id="PartialVolumeClassifier_1.Lambda" name="Lambda" description="neighborhood constant (default: 0.1)&#xA;increase for smoother boundaries&#xA;decrease if labels are smeared&#xA;" required="false" enabled="true" order="3" switch="-l" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                                <values>
                                        <value>0.1</value>
                                </values>
                        </input>
                        <output id="PartialVolumeClassifier_1.FractionFile" name="Fraction File" description="floating point output file of tissue fractions&#xA;1  (CSF) &lt;-&gt; 2 (GM) &lt;-&gt; 3 (WM)&#xA;" required="false" enabled="true" order="4" switch="-f" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Analyze Image" extension="img" description="Analyze Image file">
                                                        <need>hdr</need>
                                                </filetype>
                                        </fileTypes>
                                </format>
                        </output>
                        <input id="PartialVolumeClassifier_1.ThreeClassLabels" name="Three Class Labels" description="use a three-class (CSF=0,GM=1,WM=2) labeling" required="false" enabled="false" order="5" switch="-3" switchSpaced="true">
                                <format type="String" cardinality="0" />
                        </input>
                        <output id="PartialVolumeClassifier_1.LabelFile" name="Label File" description="Specify Label output path" required="true" enabled="true" order="1" switch="-o" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Analyze Image" extension="img" description="Analyze Image file">
                                                        <need>hdr</need>
                                                </filetype>
                                        </fileTypes>
                                </format>
                        </output>
                        <input id="PartialVolumeClassifier_1.Verbosity" name="Verbosity" description="verbosity level (0 = silent) [default: 1]" required="false" enabled="true" order="6" switch="-v" switchSpaced="true">
                                <format type="Enumerated" cardinality="1">
                                        <enumeration>0</enumeration>
                                        <enumeration>1</enumeration>
                                        <enumeration>2</enumeration>
                                </format>
                                <values>
                                        <value>2</value>
                                </values>
                        </input>
                        <input id="PartialVolumeClassifier_1.Timer" name="Timer" description="Time processing" required="false" enabled="true" order="7" switch="--timer" switchSpaced="true">
                                <format type="String" cardinality="0" />
                        </input>
                </module>
