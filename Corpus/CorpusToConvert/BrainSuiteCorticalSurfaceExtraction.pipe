                        <input id="Pharm_1.Tolerance" name="Tolerance" description="convergence tolerance [default: 1e-08]" required="false" enabled="false" order="2" switch="-t" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="Pharm_1.ComputeP-HarmonicMap" name="Compute P-Harmonic Map" description="compute p-harmonic map [default is p=2, must be even, positive] [default: 2]" required="false" enabled="false" order="3" switch="-p" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="Pharm_1.MaxIterations" name="Max Iterations" description="max iterations [default: 1000]" required="false" enabled="false" order="4" switch="-n" switchSpaced="true">
                                <format type="Number" cardinality="1" />
