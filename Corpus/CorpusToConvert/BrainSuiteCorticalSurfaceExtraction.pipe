                        <input id="Pharm_1.MapSurfaceBorder-Uniform" name="Map Surface Border - Uniform" description="map the surface border using uniform speed" required="false" enabled="false" order="8" switch="--uniform" switchSpaced="true">
                                <format type="String" cardinality="0" />
