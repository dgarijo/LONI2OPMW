                        <input id="SurfaceGenerator_1.Input3DVolume" name="Input 3D Volume" description="Specify 3D volume path" required="true" enabled="true" order="0" switch="-i" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Analyze Image" extension="img" description="Analyze Image file">
                                                        <need>hdr</need>
                                                </filetype>
                                        </fileTypes>
                                </format>
                        </input>
                        <output id="SurfaceGenerator_1.OutputSurface" name="Output Surface" description="Output surface mesh file" required="true" enabled="true" order="1" switch="-o" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="dfs" extension="dfs" description="" />
                                        </fileTypes>
                                </format>
                        </output>
                        <input id="SurfaceGenerator_1.ImageVolume" name="Image Volume" description="shade surface model with data from image volume" required="false" enabled="false" order="2" switch="-c" switchSpaced="true">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="SurfaceGenerator_1.SmoothingIterations" name="Smoothing Iterations" description="number of smoothing iterations [default: 10]" required="false" enabled="true" order="3" switch="-n" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                                <values>
                                        <value>0</value>
                                </values>
                        </input>
                        <input id="SurfaceGenerator_1.SmoothingConstant" name="Smoothing Constant" description="smoothing constant [default: 0.5]" required="false" enabled="false" order="4" switch="-a" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="SurfaceGenerator_1.ScalingPercentile" name="Scaling Percentile" description="scaling percentile [default: 0.999]" required="false" enabled="false" order="5" switch="-f" switchSpaced="true">
