                        <input id="Pharm_1.MapSurfacetoUnitCircle" name="Map Surface to Unit Circle" description="map surface to the unit circle" required="false" enabled="false" order="7" switch="--circle" switchSpaced="true">
                                <format type="String" cardinality="0" />
