                        <input id="Pharm_1.MapSurfaceBorder-Equidistant" name="Map Surface Border - Equidistant" description="map the surface border using equidistant speed" required="false" enabled="false" order="9" switch="--equidistant" switchSpaced="true">
                                <format type="String" cardinality="0" />
