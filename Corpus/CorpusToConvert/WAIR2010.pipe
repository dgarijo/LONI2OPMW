                        <input id="AlignLinear_1.StaticPartitioningAIR3.0compatible" name="Static Partitioning (AIR 3.0 compatible)" description="Forces the use of static partitioning, reverting to the default behavior of AIR 3.0x." required="false" enabled="false" order="21" prefix="-d" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0"/>
                        </input>
                        <input id="AlignLinear_1.Non-interactionofSpatialParameterDerivatives" name="Non-interaction of Spatial Parameter Derivatives" description="Assumes a non-interaction of spatial parameter derivatives (reduces likelihood of non-positive definite Hessian matrices)." required="false" enabled="false" order="22" prefix="-q" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0"/>
