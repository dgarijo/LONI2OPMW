                        <input id="BiasFieldCorrector_0.InputFile" name="Input File" description="The filename of the input MRI file in Analyze format." required="true" enabled="true" order="0" switch="-i" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Analyze Image" extension="img" description="Analyze Image file">
                                                        <need>hdr</need>
                                                </filetype>
                                        </fileTypes>
                                </format>
                        </input>
                        <output id="BiasFieldCorrector_0.OutputFile" name="Output File" description="The filename for the corrected file." required="true" enabled="true" order="1" switch="-o" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Analyze Image" extension="img" description="Analyze Image file">
                                                        <need>hdr</need>
                                                </filetype>
                                        </fileTypes>
                                </format>
                        </output>
                        <input id="BiasFieldCorrector_0.MaskFile" name="Mask File" description="mask the input image with the file &lt;maskfile&gt;" required="false" enabled="false" order="2" switch="-m" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Analyze Image 3D" extension="img" description="Analyze Image 3D file">
                                                        <need>hdr</need>
                                                </filetype>
                                        </fileTypes>
                                </format>
                        </input>
                        <input id="BiasFieldCorrector_0.HistogramRadius" name="Histogram Radius" description="histogram radius (voxels) [default: 12]" required="false" enabled="false" order="5" switch="-r" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="BiasFieldCorrector_0.BiasSampleSpacing" name="Bias Sample Spacing" description="bias sample spacing (voxels) [default: 16]" required="false" enabled="false" order="6" switch="-s" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="BiasFieldCorrector_0.ControlPointSpacing" name="Control Point Spacing" description="control point spacing (voxels) [default: 64]" required="false" enabled="false" order="7" switch="-c" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="BiasFieldCorrector_0.SplineStiffnessWeighting" name="Spline Stiffness Weighting" description="spline stiffness weighting parameter [default: 1e-04]" required="false" enabled="false" order="8" switch="-w" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="BiasFieldCorrector_0.Minimumallowedbiasestimate" name="Minimum allowed bias estimate" description="Minimum allowed bias value (default 0.9)" required="false" enabled="false" order="13" switch="-L" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="BiasFieldCorrector_0.Maximumallowedbiasestimate" name="Maximum allowed bias estimate" description="maximum allowed bias estimate (default 1.1)" required="false" enabled="false" order="14" switch="-U" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <output id="BiasFieldCorrector_0.MaskedBiasOutput" name="Masked Bias Output" description="specify masked bias field estimate output path" required="false" enabled="false" order="4" switch="--maskedbias" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="File" extension="" description="Any type of data file" />
                                        </fileTypes>
                                </format>
                        </output>
                        <input id="BiasFieldCorrector_0.Ellipse" name="Ellipse" description="use ellipsoid for ROI histogram" required="false" enabled="false" order="9" switch="--ellipse" switchSpaced="true">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="BiasFieldCorrector_0.Block" name="Block" description="Use block for ROI histogram" required="false" enabled="false" order="10" switch="--block" switchSpaced="true">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="BiasFieldCorrector_0.IterativeMode" name="Iterative Mode" description="iterative mode (overrides all other settings)" required="false" enabled="false" order="11" switch="--iterate" switchSpaced="true">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="BiasFieldCorrector_0.ApplyCorrectionField" name="Apply Correction Field" description="apply correction field to entire volume" required="false" enabled="false" order="12" switch="--extrapolate" switchSpaced="true">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="BiasFieldCorrector_0.SmallBiasModel" name="Small Bias Model" description="small bias model [0.95,1.05]" required="false" enabled="false" order="15" switch="--low" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="BiasFieldCorrector_0.MediumBiasModel" name="Medium Bias Model" description="medium bias model [0.90,1.10]" required="false" enabled="false" order="16" switch="--medium" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="BiasFieldCorrector_0.HighBiasModel" name="High Bias Model" description="high bias model [0.80,1.20]" required="false" enabled="false" order="17" switch="--high" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="BiasFieldCorrector_0.Verbosity" name="Verbosity" description="verbosity level (0=silent) [default: 1]" required="false" enabled="false" order="18" switch="-v" switchSpaced="true">
                                <format type="Enumerated" cardinality="1">
                                        <enumeration>0</enumeration>
                                        <enumeration>1</enumeration>
                                </format>
                                <values>
                                        <value>1</value>
                                </values>
                        </input>
                        <input id="BiasFieldCorrector_0.ShowTiming" name="Show Timing" description="Display timing information" required="false" enabled="false" order="19" switch="--timer" switchSpaced="true">
                                <format type="String" cardinality="0" />
                        </input>
                        <output id="BiasFieldCorrector_0.BiasOutput" name="Bias Output" description="save bias field estimate" required="false" enabled="false" order="3" switch="--bias" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="File" extension="" description="Any type of data file" />
                                        </fileTypes>
                                </format>
                        </output>
