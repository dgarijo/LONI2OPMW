                                <input id="KL_MI_register_0.Gaussiansmoothing" name="Gaussian smoothing" description="gaussian smoothing of all data with specified standard deviation. If salt and pepper noise is prevalent in your images, MI will likely fail without this. Good std - 0.4" required="false" enabled="false" order="1" switch="-g" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                        <values>
                                                <value>0.5</value>
                                        </values>
                                </input>
                                <output id="KL_MI_register_0.Usedatlas" name="Used atlas" description="Analyze file containing the version of the atlas that is on the same grid as the native displacement fields. NOTE: if the regrid displacements option is used, this may no longer be on the same grid -- the field itself is put on the original atlas or template grid and padded with zeros if needed" required="false" enabled="false" order="6" switch="-a " switchSpaced="true">
                                        <format type="File" cardinality="1">
                                                <fileTypes>
                                                        <filetype name="Analyze Image 3D" extension="img" description="Analyze Image 3D file">
                                                                <need>hdr</need>
                                                        </filetype>
                                                </fileTypes>
                                        </format>
                                </output>
                                <input id="KL_MI_register_0.regriddisplacementsflag" name="regrid displacements flag" description="1 -- field is printed on the template(data) grid&#xA;2 -- field is printed on the atlas grid &#xA;&#xA;zero padding is used when needed" required="false" enabled="false" order="7" switch="-r " switchSpaced="true">
                                        <format type="Enumerated" cardinality="1">
                                                <enumeration>1</enumeration>
                                                <enumeration>2</enumeration>
                                        </format>
                                        <values>
                                                <value>1</value>
                                        </values>
                                </input>
                                <input id="KL_MI_register_0.triangleu" name="triangle u" description="Maximum displacement for determining variable time step. Default = 0.1" required="false" enabled="true" order="2" switch="-u" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                        <values>
                                                <value>0.5</value>
                                        </values>
                                </input>
                                <input id="KL_MI_register_0.MutualInformationgaussianstd" name="Mutual Information gaussian std" description="default - 6.0" required="false" enabled="false" order="5" switch="-m" switchSpaced="true">
