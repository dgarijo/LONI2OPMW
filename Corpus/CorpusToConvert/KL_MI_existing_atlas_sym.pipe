                                        <values>
                                                <value>4.5</value>
                                        </values>
                                </input>
                                <input id="KL_MI_register_0.MedianFilter" name="Median Filter" description="Preprocess with 3x3x3 median filter. &#xA;1. data (template)&#xA;2. atlas (target)&#xA;3. both" required="false" enabled="false" order="3" switch="-p" switchSpaced="true">
                                        <format type="Enumerated" cardinality="1">
