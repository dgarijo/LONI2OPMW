                                </input>
                                <input id="Reslice_0.RetainStandardVolumesVoxelDimensions" name="Retain Standard Volume's Voxel Dimensions" description="Does not allow cubic interpolation of voxels in the target space, but instead retains the voxel dimensions of the standard file that defines the target space." required="false" enabled="false" order="5" switch="-k" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="Reslice_0.InterpolationModel2,3,4" name="Interpolation Model 2,3,4" description="The type of interpolation to be used to create the new volume:&#xA;&#xA;2: Windowed sinc in the original XY plane, linear along Z.  (requires the X- and Y-half-window-widths be provided).&#xA;3: Windowed sinc in the original XZ plane, linear along Y.  (requires the X- and Z-half-window-widths be provided).&#xA;4: Windowed sinc in the original YZ plane, linear along X.  (requires the Y- and Z-half-window-widths be provided).&#xA;" required="false" enabled="false" order="6" switch="-n" switchSpaced="true">
                                        <format type="Enumerated" cardinality="1">
                                                <enumeration>2</enumeration>
                                                <enumeration>3</enumeration>
                                                <enumeration>4</enumeration>
                                        </format>
                                        <dependencies>
                                                <dependent>Half-window-width values for Interpolation Models 2-4</dependent>
                                                <dependsOn>Half-window-width values for Interpolation Models 2-4</dependsOn>
                                        </dependencies>
                                </input>
                                <input id="Reslice_0.Overwrite" name="Overwrite" description="grants permission to overwrite output" required="false" enabled="false" order="11" switch="-o" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="Reslice_0.MultiplybyIntensityScaleFactor" name="Multiply by Intensity Scale Factor" description="Change the output intensities of the produced volume by mutiplying by this scale factor." required="false" enabled="false" order="12" switch="-s" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="Reslice_0.MultiplicativeIntensityScaleFactorFile" name="Multiplicative Intensity Scale Factor File" description="output intensities are multiplied by the value in this ASCII file containing a single positive number " required="false" enabled="false" order="13" switch="-sf" switchSpaced="true">
                                        <format type="File" cardinality="1">
                                                <fileTypes>
                                                        <filetype name="Text file" extension="txt" description="Text file" />
                                                </fileTypes>
                                        </format>
                                </input>
                                <input id="Reslice_0.AlternateX-dimensionsizeshift" name="Alternate X-dimension/size/shift" description="the shift (in voxels) to be applied along the X dimension, with positive and negative values causing shifts in opposite directions &#xA;&#xA;OR&#xA;&#xA;the new desired voxel size along the X dimension (in whatever unit was used for sizes in your image files, usually millimeters) &#xA;" required="false" enabled="false" order="14" switch="-x" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="Reslice_0.AlternateY-dimensionsizeshift" name="Alternate Y-dimension/size/shift" description="the shift (in voxels) to be applied along the Y dimension, with positive and negative values causing shifts in opposite directions &#xA;&#xA;OR&#xA;&#xA;the new desired voxel size along the Y dimension (in whatever unit was used for sizes in your image files, usually millimeters) &#xA;" required="false" enabled="false" order="15" switch="-y" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="Reslice_0.AlternateZ-dimensionsizeshift" name="Alternate Z-dimension/size/shift" description="the shift (in voxels) to be applied along the Z dimension, with positive and negative values causing shifts in opposite directions &#xA;&#xA;OR&#xA;&#xA;the new desired voxel size along the Z dimension (in whatever unit was used for sizes in your image files, usually millimeters) &#xA;" required="false" enabled="false" order="16" switch="-z" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="Reslice_0.Half-window-widthvaluesforInterpolationModels2-4" name="Half-window-width values for Interpolation Models 2-4" required="false" enabled="false" order="9">
                                        <format type="Number" cardinality="2" />
                                        <dependencies>
                                                <dependent>Interpolation Model 2,3,4</dependent>
                                                <dependsOn>Interpolation Model 2,3,4</dependsOn>
                                        </dependencies>
                                </input>
                                <input id="Reslice_0.InterpolationModel0,1,7,10-13" name="Interpolation Model 0, 1, 7, 10-13" description="The type of interpolation to be used to create the new volume:&#xA;&#xA;0:  Nearest neighbor (should be used for enumerated value files where only specific values have meanings and should be available).&#xA;1: Trilinear&#xA;7: 3D unwindowed scanline sinc.&#xA;10: 3D scanline chirp-z.&#xA;11: scanline chirp-z in the original XY plane, linear along the Z-axis.&#xA;12: scanline chirp-z in the original XZ plane, linear along the Y-axis.&#xA;13: scanline chirp-z in the original YZ plane, linear along the X-axis." required="false" enabled="false" order="8" switch="-n" switchSpaced="true">
                                        <format type="Enumerated" cardinality="1">
                                                <enumeration>0</enumeration>
                                                <enumeration>1</enumeration>
                                                <enumeration>7</enumeration>
                                                <enumeration>10</enumeration>
                                                <enumeration>11</enumeration>
                                                <enumeration>12</enumeration>
                                                <enumeration>13</enumeration>
                                        </format>
                                </input>
                                <input id="Reslice_0.InterpolationModel5,6" name="Interpolation Model 5,6" description="The type of interpolation to be used to create the new volume:&#xA;&#xA;5: 3D windowed sinc.  (requires the X-, Y-, and Z-half-window-widths be provided).&#xA;6: 3D windowed scanline sinc.  (requires the X-, Y-, and Z-half-window-widths be provided).&#xA;" required="false" enabled="false" order="7" switch="-n" switchSpaced="true">
                                        <format type="Enumerated" cardinality="1">
                                                <enumeration>5</enumeration>
                                                <enumeration>6</enumeration>
                                        </format>
                                        <dependencies>
                                                <dependent>Half-window-width values for Interpolation Models 5,6</dependent>
                                                <dependsOn>Half-window-width values for Interpolation Models 5,6</dependsOn>
                                        </dependencies>
                                </input>
                                <input id="Reslice_0.Half-window-widthvaluesforInterpolationModels5,6" name="Half-window-width values for Interpolation Models 5,6" required="false" enabled="false" order="10">
                                        <format type="Number" cardinality="3" />
                                        <dependencies>
                                                <dependent>Interpolation Model 5,6</dependent>
                                                <dependsOn>Interpolation Model 5,6</dependsOn>
                                        </dependencies>
                                </input>
                        </module>
                        <module id="AlignLinear_0" name="Align Linear" package="AIR" version="5.2.5" description="This is a general linear intramodality registration tool (within or across subjects, 2D or 3D). The user can specify any of a variety of models, including rigid-body, affine, or perspective.&#xA;&#xA;The program will generate a .air file that can be used to reslice the specified reslice data set to match the specified standard data set.&#xA;&#xA;64-bit compilation" location="pipeline://cranium.loni.usc.edu//usr/local/loniWorkflows/MinimumDistanceTemplate/csvarer/alignlinear" sourceCode="" icon="/9j/2wBDAAgGBgcGBQgHBwcJCQgKDBQNDAsLDBkSEw8UHRofHh0aHBwgJC4nICIsIxwcKDcpLDAx&#xA;NDQ0Hyc5PTgyPC4zNDL/2wBDAQkJCQwLDBgNDRgyIRwhMjIyMjIyMjIyMjIyMjIyMjIyMjIyMjIy&#xA;MjIyMjIyMjIyMjIyMjIyMjIyMjIyMjIyMjL/wAAUCAA4ADgEASIAAhEBAxEBBCIA/8QAHwAAAQUB&#xA;AQEBAQEAAAAAAAAAAAECAwQFBgcICQoL/8QAtRAAAgEDAwIEAwUFBAQAAAF9AQIDAAQRBRIhMUEG&#xA;E1FhByJxFDKBkaEII0KxwRVS0fAkM2JyggkKFhcYGRolJicoKSo0NTY3ODk6Q0RFRkdISUpTVFVW&#xA;V1hZWmNkZWZnaGlqc3R1dnd4eXqDhIWGh4iJipKTlJWWl5iZmqKjpKWmp6ipqrKztLW2t7i5usLD&#xA;xMXGx8jJytLT1NXW19jZ2uHi4+Tl5ufo6erx8vP09fb3+Pn6/8QAHwEAAwEBAQEBAQEBAQAAAAAA&#xA;AAECAwQFBgcICQoL/8QAtREAAgECBAQDBAcFBAQAAQJ3AAECAxEEBSExBhJBUQdhcRMiMoEIFEKR&#xA;obHBCSMzUvAVYnLRChYkNOEl8RcYGRomJygpKjU2Nzg5OkNERUZHSElKU1RVVldYWVpjZGVmZ2hp&#xA;anN0dXZ3eHl6goOEhYaHiImKkpOUlZaXmJmaoqOkpaanqKmqsrO0tba3uLm6wsPExcbHyMnK0tPU&#xA;1dbX2Nna4uPk5ebn6Onq8vP09fb3+Pn6/9oADgQBAAIRAxEEAAA/APn+iug/4mnjjXP+WCzLF/up&#xA;Gg/Mnk+/X06anhjwumravDFejZBIzqJHyWJC7ySuQ2OwOBnnuCA4xbdkDdj5/oorpvCnhafxBfvF&#xA;JbXPkiIsJEXAzkdyMdzXF0V6+/wk+ZmhvZJnYYEkgMZXp0ySegxyOhqa2+Ft3DdwTzXpk8h/MjXz&#xA;i2HyDu5GM8D8hQ0lu0NJs5mivWV+DTS8/bngH+3hj+lXrb4L2UeGuNTlnP8AdCbB/MmvGq2JkkVE&#xA;a7aZxLIrSMVKfKFwE3Hpjlfu4GBjIr2yx8FaVYh92lzyFzlgGQK3XkqpAPU/TNZPjnQjdaTFDpOk&#xA;usi7gESAjphh93n+H1xUxlGUuVblyp2jzNnjFTta3CQCdoJVhY4DlCFJ9M17xa/DrTrDBt7K1Yjo&#xA;0mWb9ayPHfhe/uPDgW3tkYxTK4VGUeo9vWvEaK6688OXunrJbPZxzCUK0lysXAx0VQ6xlPc9+PfJ&#xA;Wns59mY80