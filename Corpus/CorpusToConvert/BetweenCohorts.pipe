                                        <format type="Number" cardinality="3" />
                                </input>
                                <input id="MRIConvert_1.FlipAngle" name="Flip Angle" description="Flip Angle in radians.&#xA;Must specify &lt;flip_angle&gt;." required="false" enabled="false" order="34" switch="-flip_angle" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="MRIConvert_1.OutputjCount" name="Output j Count" description="Specifies the output &lt;j count&gt;." required="false" enabled="false" order="16" switch="-onj" switchSpaced="true">
