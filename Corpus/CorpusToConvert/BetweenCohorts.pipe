                                <input id="VolumeTwoIndependentSamplesT-Test_1.DesignMatrix" name="Design Matrix" description="Enter Design Matrix. See the template here:&#xA;&#xA;http://wiki.stat.ucla.edu/socr/index.php/SOCR_EduMaterials_AnalysesCommandLineVolume2Samples_T_test" required="true" enabled="true" order="0" switch="-dm" switchSpaced="true">
                                        <format type="File" cardinality="1">
                                                <fileTypes>
                                                        <filetype name="Text file" extension="txt" description="Text file" />
                                                </fileTypes>
                                        </format>
                                </input>
                                <input id="VolumeTwoIndependentSamplesT-Test_1.RegressorsSpecification" name="Regressors Specification" description="Regressor Specifications.&#xA;&#xA;One Regressor must be specified from the list of predictors/columns/covariates listed in the Design Matrix (see first argument). You must specify 2 values for this predictor that will be used to extract the image files that represent the 2 groups (no spaces!)&#xA;&#xA;Example:&#xA;-regressors GROUP=1,2" required="true" enabled="true" order="2" switch="-regressors" switchSpaced="true">
                                        <format type="String" cardinality="1" />
                                        <values>
                                                <value>GROUP=0,1</value>
                                        </values>
                                </input>
                                <input id="VolumeTwoIndependentSamplesT-Test_1.HeaderFlag" name="Header Flag" description="Header Flag (-h).&#xA;&#xA;Required for all Design Matrices." required="true" enabled="true" order="1" switch="-h" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="VolumeTwoIndependentSamplesT-Test_1.VolumeDimensions" name="Volume Dimensions" description="Specify the Z Volume Dimension&#xA;Orders is: (Z, Y, X):&#xA;&#xA;Example:&#xA;-dim 220 220 220&#xA;&#xA;For 1D or 2D data, use 1 for the higher demensions." required="true" enabled="true" order="3" switch="-dim" switchSpaced="true">
                                        <format type="Number" cardinality="3" />
                                </input>
                                <input id="VolumeTwoIndependentSamplesT-Test_1.DataType" name="Data Type" description="Specify the Input Data type:&#xA;&#xA;Example: -data_type 2&#xA;-data_type [1,2,3,4]: &#xA;Type=1 is for Byte, &#xA;Type=2 is for Unsigned Short integer, &#xA;Type=3 is for Signed Short Integer, and &#xA;Type=4 is for 4Byte=Float Volume Input;&#xA;" required="true" enabled="true" order="6" switch="-data_type" switchSpaced="true">
                                        <format type="Enumerated" cardinality="1">
                                                <enumeration>1</enumeration>
                                                <enumeration>2</enumeration>
                                                <enumeration>3</enumeration>
                                                <enumeration>4</enumeration>
                                        </format>
                                        <values>
                                                <value>2</value>
                                        </values>
                                </input>
                                <input id="VolumeTwoIndependentSamplesT-Test_1.MaskFile" name="Mask File" description="Enter an optional Mask File. It is used to restrict the voxel-based domain of the Regression Analysis.&#xA;&#xA;If no Mask is specified all voxes are processed independently.&#xA;&#xA;Example:&#xA;-mask /ifs/adni/3T/PERMS/IVO/JACOBIANS/3T_bl_002_S_0413.img &#xA;" required="false" enabled="true" order="5" switch="-mask" switchSpaced="true">
                                        <format type="File" cardinality="1">
                                                <fileTypes>
                                                        <filetype name="Analyze Image" extension="img" description="Analyze Image">
                                                                <need>hdr</need>
                                                        </filetype>
                                                </fileTypes>
                                        </format>
                                </input>
                                <input id="VolumeTwoIndependentSamplesT-Test_1.Byte-Order" name="Byte-Order" description="-byteorder string: string is one of {big, little, other}.&#xA;&#xA;    * big = BIG_ENDIAN processor&#xA;    * little = LITTLE_ENDIAN processor&#xA;    * other = default processor (java.nio.ByteOrder.nativeOrder()) " required="false" enabled="true" order="7" switch="-byteorder" switchSpaced="true">
                                        <format type="Enumerated" cardinality="1">
                                                <enumeration>little</enumeration>
                                                <enumeration>big</enumeration>
                                                <enumeration>default</enumeration>
                                        </format>
                                        <values>
                                                <value>little</value>
                                        </values>
                                </input>
                                <output id="VolumeTwoIndependentSamplesT-Test_1.P-valueOutputVolume" name="P-value Output Volume" description="This should be the same value as the Output P-value parameter.&#xA;&#xA;This is a pipeline workaround!" required="false" enabled="true" order="8" switch="-p" switchSpaced="true">
                                        <format type="File" cardinality="1">
                                                <fileTypes>
                                                        <filetype name="Analyze Image" extension="img" description="Analyze Image file">
