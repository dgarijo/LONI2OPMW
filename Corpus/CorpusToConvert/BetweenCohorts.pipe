                                <input id="FSLMathsnii.gz_26.Grid" name="Grid" description="Add a 3D grid of intensity &lt;value&gt; with grid spacing &lt;spacing&gt;.&#xA;Usage: -grid &lt;value&gt; &lt;spacing&gt;" required="false" enabled="false" order="29" switch="-grid" switchSpaced="true">
                                        <format type="Number" cardinality="2" />
                                </input>
                                <input id="FSLMathsnii.gz_26.TFCE" name="TFCE" description="Enhance with TFCE, e.g. -tfce 2 0.5 6 (maybe change 6 to 26 for skeletons).&#xA;Usage: -tfce &lt;H&gt; &lt;E&gt; &lt;connectivity&gt;" required="false" enabled="false" order="31" switch="-tfce" switchSpaced="true">
                                        <format type="Number" cardinality="3" />
                                </input>
                                <input id="FSLMathsnii.gz_26.IntensityNormalisationper3Dvolumemean" name="Intensity Normalisation (per 3D volume mean)" description="(-i i ip.c) intensity normalisation (per 3D volume mean).&#xA;Usage: -inm &lt;mean&gt;" required="false" enabled="false" order="34" switch="-inm" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="FSLMathsnii.gz_26.IntensityNormalisationglobal4Dmean" name="Intensity Normalisation (global 4D mean)" description="(-I i ip.c) intensity normalisation, global 4D mean).&#xA;Usage: -ing &lt;mean&gt;" required="false" enabled="false" order="35" switch="-ing" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="FSLMathsnii.gz_26.Kernel3D" name="Kernel 3D" description="3x3x3 box centered on target voxel (set as default kernel)." required="false" enabled="false" order="36" switch="-kernel 3D" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.Kernel2D" name="Kernel 2D" description="3x3x1 box centered on target voxel." required="false" enabled="false" order="37" switch="-kernel 2D" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.KernelBox" name="Kernel Box" description="All voxels in a box of width &lt;size&gt; centered on target voxel.&#xA;Usage: -kernel box &lt;size&gt;" required="false" enabled="false" order="38" switch="-kernel box" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="FSLMathsnii.gz_26.KernelBoxV" name="Kernel BoxV" description="&lt;size&gt;x&lt;size&gt;x&lt;size&gt; box centered on target voxel. &#xA;CAUTION: size should be an odd number.&#xA;Usage: -kernel boxv &lt;size&gt;" required="false" enabled="false" order="39" switch="-kernel boxv" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="FSLMathsnii.gz_26.KernelGauss" name="Kernel Gauss" description="Gaussian kernel (sigma in mm, not voxels).&#xA;Usage: -kernel gauss &lt;sigma&gt;" required="false" enabled="false" order="40" switch="-kernel gauss" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="FSLMathsnii.gz_26.KernelSphere" name="Kernel Sphere" description="All voxels in a sphere of radius &lt;size&gt; mm centered on target voxel.&#xA;Usage: -kernel sphere &lt;size&gt;" required="false" enabled="false" order="41" switch="-kernel sphere" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="FSLMathsnii.gz_26.KernelFile" name="Kernel File" description="Use external file as kernel.&#xA;Usage: -kernel file &lt;filename&gt;" required="false" enabled="false" order="42" switch="-kernel file" switchSpaced="true">
                                        <format type="File" cardinality="1">
                                                <fileTypes>
                                                        <filetype name="File" extension="" description="Any type of data file" />
                                                </fileTypes>
                                        </format>
                                </input>
                                <input id="FSLMathsnii.gz_26.MeanDilation" name="Mean Dilation" description="Mean Dilation of zero voxels  (using non-zero voxels in kernel)." required="false" enabled="false" order="43" switch="-dilM" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.ModalDilation" name="Modal Dilation" description="Modal Dilation of zero voxels (using non-zero voxels in kernel)." required="false" enabled="false" order="44" switch="-dilD" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.MaximumFiltering" name="Maximum Filtering" description="Maximum filtering of all voxels." required="false" enabled="false" order="45" switch="-dilF" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.Erode" name="Erode" description="Erode by zeroing non-zero voxels when zero voxels found in kernel." required="false" enabled="false" order="46" switch="-ero" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.MinimumFiltering" name="Minimum Filtering" description="Minimum filtering of all voxels." required="false" enabled="false" order="47" switch="-eroF" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.MedianFiltering" name="Median Filtering" description="Median filtering." required="false" enabled="false" order="48" switch="-fmedian" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.MeanFiltering" name="Mean Filtering" description="Mean filtering, kernel weighted (conventionally used with gauss kernel)." required="false" enabled="false" order="49" switch="-fmean" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.MeanFilteringun-normalised" name="Mean Filtering (un-normalised)" description="Mean filtering, kernel weighted, un-normalised (gives edge effects)." required="false" enabled="false" order="50" switch="-fmeanu" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.Sigma" name="Sigma" description="Create a gauss kernel of sigma mm and perform mean filtering.&#xA;Usage: -s &lt;sigma&gt;" required="false" enabled="false" order="51" switch="-s" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                                <input id="FSLMathsnii.gz_26.DownsampleImagecentered" name="Downsample Image (centered)" description="Downsamples image by a factor of 2 (keeping new voxels centred on old)." required="false" enabled="false" order="52" switch="-subsamp2" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.DownsampleImagenon-centered" name="Downsample Image (non-centered)" description="Downsamples image by a factor of 2 (non-centred)." required="false" enabled="false" order="53" switch="-subsamp2offc" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.RegionofInterest" name="Region of Interest" description="Zero outside ROI.&#xA;Usage: -roi &lt;xmin&gt; &lt;xsize&gt; &lt;ymin&gt; &lt;ysize&gt; &lt;zmin&gt; &lt;zsize&gt; &lt;tmin&gt; &lt;tsize&gt;" required="false" enabled="false" order="86" switch="-roi" switchSpaced="true">
                                        <format type="Number" cardinality="8" />
                                </input>
                                <input id="FSLMathsnii.gz_26.BPTF" name="BPTF" description="(-t in ip.c)  Bandpass temporal filtering; nonlinear highpass and Gaussian linear lowpass (with sigmas in volumes, not seconds); set either sigma&lt;0 to skip that filter.&#xA;Usage: -bptf &lt;hp_sigma&gt; &lt;lp_sigma&gt;" required="false" enabled="false" order="87" switch="-bptf" switchSpaced="true">
                                        <format type="Number" cardinality="2" />
                                </input>
                                <input id="FSLMathsnii.gz_26.ROC" name="ROC" description="Take (normally binary) truth and test current image in ROC analysis against truth. &lt;AROC-thresh&gt; is usually 0.05 and is limit of Area-under-ROC measure FP axis. &lt;outfile&gt; is a text file of the ROC curve (triplets of values: FP TP threshold).  If the truth image contains negative voxels these get excluded from all calculations. If &lt;AROC-thresh&gt; is positive then the [4Dnoiseonly] option needs to be set, and the FP rate is determined from this noise-only data, and is set to be the fraction of timepoints where any FP (anywhere) is seen, as found in the noise-only 4d-dataset.  This is then controlling the FWE rate.  If &lt;AROC-thresh&gt; is negative the FP rate is calculated from the zero-value parts of the &lt;truth&gt; image, this time averaging voxelwise FP rate over all timepoints. In both cases the TP rate is the average fraction of truth=positive voxels correctly found.&#xA;Usage: -roc &lt;AROC-thresh&gt; &lt;outfile&gt; [4dnoiseonly] &lt;truth&gt;" required="false" enabled="false" order="88" switch="-roc" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                        <dependencies>
                                                <dependent>4D Noise Only</dependent>
                                                <dependent>AROC-thresh</dependent>
                                        </dependencies>
                                </input>
                                <input id="FSLMathsnii.gz_26.4DNoiseOnly" name="4D Noise Only" description="Option for ROC.  If &lt;AROC-thresh&gt; is positive then the [4Dnoiseonly] option needs to be set." required="false" enabled="false" order="91" switch="4Dnoiseonly" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                        <dependencies>
                                                <dependsOn>ROC</dependsOn>
                                        </dependencies>
                                </input>
                                <input id="FSLMathsnii.gz_26.AROC-thresh" name="AROC-thresh" description="Option for ROC.  &lt;AROC-thresh&gt; is usually 0.05 and is limit of Area-under-ROC measure FP axis." required="false" enabled="false" order="89">
                                        <format type="Number" cardinality="1" />
                                        <dependencies>
                                                <dependsOn>ROC</dependsOn>
                                        </dependencies>
                                </input>
                                <input id="FSLMathsnii.gz_26.Truth" name="Truth" description="Option for ROC.  If &lt;AROC-thresh&gt; is negative the FP rate is calculated from the zero-value parts of the &lt;truth&gt; image, this time averaging voxelwise FP rate over all timepoints." required="false" enabled="false" order="92">
                                        <format type="File" cardinality="1">
                                                <fileTypes>
                                                        <filetype name="File" extension="" description="Any type of data file" />
                                                </fileTypes>
                                        </format>
                                </input>
                                <input id="FSLMathsnii.gz_26.Outfile" name="Outfile" description="Option for ROC. &lt;outfile&gt; is a text file of the ROC curve (triplets of values: FP TP threshold)." required="false" enabled="false" order="90">
                                        <format type="File" cardinality="1">
                                                <fileTypes>
                                                        <filetype name="Text file" extension="txt" description="Text file" />
                                                </fileTypes>
                                        </format>
                                </input>
                                <input id="FSLMathsnii.gz_26.YMin" name="Y Min" description="Minimum across y dimension." required="false" enabled="false" order="76" switch="-Ymin" switchSpaced="true">
                                        <format type="String" cardinality="0" />
                                </input>
                                <input id="FSLMathsnii.gz_26.Addinteger" name="Add (integer)" description="Add following input (integer) to current image." required="false" enabled="false" order="1" switch="-add" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
                        </module>
