                                </input>
                                <input id="MRIConvert_2.FlipAngle" name="Flip Angle" description="Flip Angle in radians.&#xA;Must specify &lt;flip_angle&gt;." required="false" enabled="false" order="34" switch="-flip_angle" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                </input>
