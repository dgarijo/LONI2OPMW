                        <input id="dcm2img_2.input" name="input" required="true" enabled="true" order="1" prefix="-i" prefixSpaced="true" prefixAllArgs="false">
                                <format type="File" cardinality="-1">
                                        <fileTypes>
                                                <filetype name="DICOM file" extension="dcm" description="A type of DICOM image." />
                                        </fileTypes>
                                </format>
                        </input>
                        <input id="dcm2img_2.move" name="move" description="move files to subdirectories" required="false" enabled="false" order="2" prefix="--move" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="dcm2img_2.timer" name="timer" description="time the conversion" required="false" enabled="false" order="3" prefix="--timer" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="dcm2img_2.radiologicalorientation" name="radiological orientation" description="use radiological orientation" required="false" enabled="false" order="4" prefix="-r" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="dcm2img_2.information" name="information" description="information only" required="false" enabled="false" order="5" prefix="--info" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="dcm2img_2.LPI" name="LPI" description="rotate to LPI/RAS coordinates" required="false" enabled="true" order="6" prefix="--lpi" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="dcm2img_2.Outputnifti" name="Output nifti" description="output nifti" required="false" enabled="false" order="7" prefix="-n" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="dcm2img_2.Outputgzip" name="Output gzip" description="gzip output" required="false" enabled="false" order="8" prefix="-z" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="dcm2img_2.verbose" name="verbose" description="verbose level&#xA;0=quiet&#xA;1=some info&#xA;2=lots&#xA;[default: 0]" required="false" enabled="true" order="9" prefix="-v" prefixSpaced="true" prefixAllArgs="false">
                                <format type="Enumerated" cardinality="1">
                                        <enumeration>0</enumeration>
                                        <enumeration>1</enumeration>
                                        <enumeration>2</enumeration>
                                </format>
                                <values>
                                        <value>2</value>
                                </values>
                        </input>
                        <output id="dcm2img_2.Outputanalyzeimage" name="Output analyze image" required="true" enabled="true" order="0">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Analyze Image" extension="img" description="Analyze Image">
                                                        <need>hdr</need>
                                                </filetype>
                                        </fileTypes>
                                </format>
                        </output>
