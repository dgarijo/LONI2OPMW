                        </input>
                        <input id="SPMCoregister_1.sep" name="sep" description="Separation: the average distance between sampled points (in mm)&#xA;[4 2] default" required="false" enabled="false" order="6" prefix="sep" prefixSpaced="true" prefixAllArgs="false">
