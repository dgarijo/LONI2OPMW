                        </input>
                        <input id="SPMCoregister_1.tol" name="tol" description="Tolerances: the accuracy for each parameter. Iterations stop when differences between successive estimates are less than required tolerance.&#xA;1x12 double format&#xA;[0.02 0.02 0.02 0.001 0.001 0.001 0.01 0.01 0.01 0.001 0.001 0.001] (default)" required="false" enabled="false" order="7" prefix="tol" prefixSpaced="true" prefixAllArgs="false">
