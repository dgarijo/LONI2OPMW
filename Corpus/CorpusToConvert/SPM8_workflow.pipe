                        <input id="SPMSegment_1.warpco" name="warpco" description="Warp frequency cutoff: Cutoff of DCT bases (only DCT bases of periods longer than the cutoff are used to describe the warps)&#xA;Default=25" required="false" enabled="false" order="17" prefix="warpco" prefixSpaced="true" prefixAllArgs="false">
